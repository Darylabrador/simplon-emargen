<div class="modal fade" id="modalEmergement" tabindex="-1" role="dialog" aria-labelledby="emargementLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="emargementLabel">Nouvelle feuille d'émargement</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>

        <div id="error"> </div>

        <form id="emargementForm" method="POST">
          <input type="hidden" id="userId" name="userId" value="<%= currentUserId %>">
            <div class="modal-body">
              <div class="form-group">
                  <label for="intitule">Intitulé</label>
                  <input type="text" class="form-control" id="intitule" aria-describedby="intitule">
              </div>
              <div class="form-group">
                <label for="url">Lien du google sheet</label>
                <input type="url" class="form-control" id="url" aria-describedby="url">
              </div>
            </div>
            <div class="modal-footer">
                <button id="generateBtn" type="submit" class="btn btn-primary">Générer</button>
            </div>
        </form>
      </div>
    </div>
  </div>