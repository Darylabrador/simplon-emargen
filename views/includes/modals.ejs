<div class="modal fade" id="modalEmergement" tabindex="-1" role="dialog" aria-labelledby="emargementLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="emargementLabel">Nouvelle feuille d'émargement</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>

        <div id="error"> </div>

        <form id="emargementForm" method="POST" enctype="multipart/form-data">
            <div class="modal-body">
              <div class="form-group">
                  <label for="templateName">Choisissez votre template</label>
                  <select class="form-control" id="templateName">
                    <% templateInfo.forEach(info => { %>
                      <option value="<%= info._id %>"> <%= info.name %> </option>
                    <% }) %>
                  </select>
              </div>
              <div class="form-group">
                <label for="url">Lien du google sheet</label>
                <input required type="url" class="form-control" id="url" aria-describedby="url">
              </div>
              <div class="form-group">
                <label for="number">Numéro de la feuille</label>
                <input required type="number" steps="1" min="1" class="form-control" id="number" aria-describedby="number" placeholder="1">
              </div>
            </div>
            <div class="modal-footer">
                <button id="generateBtn" type="submit" class="btn btn-primary">Générer</button>
            </div>
        </form>
      </div>
    </div>
  </div>